func main ()
   var a,b, i,j : int
   var t : bool
   var A : array [2] of int
   var M : matrix [10,20] of float

   A[0] = t + 1;
   A[1] = 7;
   i = 0;
   while i<10 do
      j = not b;
      while j<20 do
         M[i,j] = A[i%2] + j;
         j = j+1;
      endwhile
      i = i+1;
      a[i+j] = 12;
   endwhile
   
   write M[5,8]; write "\n";
endfunc
